<!--脚本说明:
把如下代码加入<body>区域中-->
<form>
<div align="center">
<input type="BUTTON" name="FullScreen" value="全屏显示" onClick="window.open(document.location, 'big', 'fullscreen=yes')">
<input type="BUTTON" name="FullScreen" value="取消全屏" onClick="window.close()">
</div>
</form>


