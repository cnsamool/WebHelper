<script LANGUAGE="JavaScript">
function goHist(a) 
{
   history.go(a); 
}
//<!-- done hiding from old browsers -->
</script>

<form METHOD="post">
  <p><input TYPE="button" VALUE="前进   " onClick="goHist(-1)"> <input TYPE="button"
  VALUE="后退" onClick="goHist(1)"> </p>
</form>
