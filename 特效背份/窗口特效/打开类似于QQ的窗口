<SCRIPT LANGUAGE="JavaScript1.2">
function myAvailable() {
window.open('about:blank','myName', 'width=100,height='+screen.availHeight+',top='+screen.availTop+',left='+screen.availLeft);
}

</SCRIPT>
<form>
<input type=button value="打开类似于QQ的窗口" onClick="myAvailable();" name="button2">
</form>
