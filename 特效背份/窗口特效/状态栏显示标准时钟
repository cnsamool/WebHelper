
<SCRIPT language="JavaScript">

	function doClock() {

		window.setTimeout( "doClock()", 1000 );

		today = new Date();

		self.status = today.toString();

    }

</SCRIPT>
<body bgcolor="#fef4d9" onLoad="doClock()">
