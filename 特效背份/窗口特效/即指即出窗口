<SCRIPT Language="JavaScript">
function winopen () 
{
msg=open("http://samool.533.net","NewWindow","toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,copyhistory=yes,width=400,height=260");
}
</SCRIPT>



<!--第二步：将下列代码复制到<body></body>之间：-->
<body>
