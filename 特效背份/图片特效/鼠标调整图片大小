<SCRIPT LANGUAGE="JavaScript">
<!-- Begin
function resizeImage(evt,name){
newX=evt.x
newY=evt.y
eval("document."+name+".width=newX")
eval("document."+name+".height=newY")
}

//  End -->
</script>
<img src="sample.jpg"
width="300" height="401" name="image"
ondrag="resizeImage(event,'image')">
