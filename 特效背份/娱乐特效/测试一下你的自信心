function processForm(form){
var xf=0;
hc=new Array(40);
var roll_call=hc.length;
for (i=roll_call;i>0;i--)
{hc[i-1]=0};
if (form.c1[0].checked==1) hc[0]=1;
if (form.c1[1].checked==1) hc[0]=0;
if (form.c2[0].checked==1) hc[1]=0;
if (form.c2[1].checked==1) hc[1]=1;
if (form.c3[0].checked==1) hc[2]=0;
